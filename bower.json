{
  "name": "IFM Maps",
  "version": "0.1.0",
  "authors": [
    "Niels Holt <nho@fcoo.dk>"
  ],
  "moduleType": [
    "es6"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "circular-slider": "latest",
    "css-element-queries": "git://github.com/marcj/css-element-queries.git#latest",
    "detectizr": "latest",
    "global-events": "NielsHolt/global-events#latest",
    "jquery": "latest",
    "jquery-autoclick-while-pressed": "silviubogan/jquery-autoclick-while-pressed#latest",
    "jquery-base-slider": "NielsHolt/jquery-base-slider#~2.3.1",
    "jquery-mousewheel": "git://github.com/jquery/jquery-mousewheel.git#latest",
    "jquery-phrase-translator": "NielsHolt/jquery-phrase-translator#latest",
    "jquery-time-slider": "NielsHolt/jquery-time-slider#latest",
    "jquery.actual": "~1.0.17",
    "jquery.bind-first": "latest",
    "leaflet": "latest",
    "leaflet-control-box": "NielsHolt/leaflet-control-box#latest",
    "leaflet-freeze": "NielsHolt/leaflet-freeze#latest",
    "leaflet-graphicscale": "NielsHolt/leaflet-graphicscale#latest",
    "leaflet-print-rectangle": "NielsHolt/leaflet-print-rectangle#~0.0.1",
    "Leaflet.awesome-markers": "latest",
    "Leaflet.label": "git://github.com/Leaflet/Leaflet.label.git#latest",
    "Leaflet.MagnifyingGlass": "bbecquet/Leaflet.MagnifyingGlass#leaflet-0.7.2",
    "lodash": "latest",
    "malihu-custom-scrollbar-plugin": "latest",
    "mathjs": "latest",
    "modernizr": "latest",
    "moment": "latest",
    "moment-timezone": "latest",
    "normalize.css": "latest",
    "noty": "latest",
    "options-storage": "NielsHolt/options-storage#latest",
    "pagina-leaflet-minimap": "latest",
    "screenfull": "latest",
    "spin.js": "git://github.com/fgnass/spin.js.git#latest",
    "webshim": "latest"
  },

	"overrides": {
		"modernizr"											: { "main"	: "modernizr.js" },
		"jquery-autoclick-while-pressed": {	"main"	: "autoclick-while-pressed.min.js" },
		"Leaflet.MagnifyingGlass"				: { "ignore": true },
		"pagina-leaflet-minimap"				: { "main"	: ["src/Control.MiniMap.js", "src/Control.MiniMap.css"]  },
		"webshim"												: { "main"	: "js-webshim/minified/polyfiller.js" },
				
		"BOWER-PACKAGE": {
			"main": {
				"development": "file.js",
				"production": "file.min.js"
			}
		}
	},

  "devDependencies": {
    "modernizr-mixin": "latest",
    "bourbon": "latest",
    "mathsass": "latest"
  }
}
